{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learnjenkins-taskdefinition-prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "nginx:1.27-alpine",
            "portMappings": [
                {
                    "name": "nginx-port",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512 MiB",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::611538927186:role/ecsTaskExecutionRole"
}